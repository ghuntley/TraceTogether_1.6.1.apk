<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" color="#0030a1">
    <item>
        <selector>
            <item state_enabled="true">
                <shape shape="rectangle">
                    <solid color="#ffffff"/>
                    <corners radius="10dp"/>
                    <gradient startColor="#ffffff" endColor="#ffffff" angle="90"/>
                </shape>
            </item>
            <item state_enabled="false">
                <shape shape="rectangle">
                    <solid color="#ffffff"/>
                    <corners radius="10dp"/>
                    <gradient startColor="#ffffff" endColor="#ffffff" angle="90"/>
                </shape>
            </item>
        </selector>
    </item>
</ripple>
